{
  "name": "wyre-app",
  "version": "1.0.0",
  "description": "Invoice App using SQLite, Node, React",
  "main": "index.tsx",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node-dev --respawn --pretty --transpile-only src/index.tsx",
    "start-server": "nodemon server/server.js --watch server/*",
