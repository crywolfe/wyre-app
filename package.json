    "bookshelf": "^1.2.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "knex": "^0.21.17",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
